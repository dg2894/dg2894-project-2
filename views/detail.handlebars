<html lang="en">
<head>
  <meta charset="utf-8"/>
  <title>IdolMaker</title>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/react/15.2.1/react.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/react/15.2.1/react-dom.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/babel-core/5.8.34/browser.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/remarkable/1.6.2/remarkable.min.js"></script>
  <script src="/pikaday"></script>
  <link rel="shortcut icon" href="/assets/img/favicon.png"/>
  <link rel="shortcut icon" href="/assets/img/favicon.png"/>
  <link rel="stylesheet" type="text/css" href="/assets/main.css"/>
  <link rel="stylesheet" type="text/css" href="/pikaday.css">
</head>
<body>
  <nav id="top-nav">
    <div class="navlink"><a href="/">Home</a></div>
    <div class="navlink"><a href="/settingsPage">Change Password</a></div>
    <div class="navlink"><a href="/logout">Log out</a></div>
  </nav>
  <div id="flex-container">
    <div class="inner-container">
      {{#each foundIdol as |idol|}}
        <section id="makeIdol">
          <input id="chosenId" name="chosenId" value={{idol._id}}/>
        </section>
        <a class="editButton" href={'/editForm/' + idol._id}>Edit</a>
      {{/each}}
    </div>
  </div>
  <script src="/assets/detail.js"></script>
  <script src="/assets/helper.js"></script>
</body>
</html>
